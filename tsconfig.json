{
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ES2015",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "vite/client",
      "vite-plugin-pages/client"
    ],
  }
}